{
  "name": "pm2-discord",
  "version": "0.1.0",
  "description": "A PM2 module to emit events to discord",
  "apps": [
    {
      "script": "index.js",
      "merge_logs": true,
      "max_memory_restart": "200M"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FranciscoG/pm2-discord"
  },
  "keywords": [
    "pm2",
    "discord"
  ],
  "author": "Francisco <info@franciscog.com> (https://github.com/FranciscoG)",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/FranciscoG/pm2-discord/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/FranciscoG/pm2-discord/issues"
  },
  "homepage": "https://github.com/FranciscoG/pm2-discord",
  "dependencies": {
    "eslint": "^5.6.1",
    "pm2": "^3.2.2",
    "pmx": "^1.6.7",
    "request": "^2.88.0",
    "strip-ansi": "^5.0.0"
  },
  "config": {
    "discord_url": null,
    "process_name": null,
    "log": true,
    "error": true,
    "kill": true,
    "exception": true,
    "restart": true,
    "delete": false,
    "stop": true,
    "restart overlimit": true,
    "exit": true,
    "start": true,
    "online": false,
    "buffer": true,
    "buffer_seconds": 1,
    "queue_max": 100
  }
}